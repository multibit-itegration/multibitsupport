{
  "$schema": "https://json.schemastore.org/angular-cli",
  "version": 1,
  "projects": {
  "task-rating-frontend": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
            "options": {
            "outputPath": "dist/task-rating-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "baseHref": "/"
          },
            "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "baseHref": "/",
              "budgets": []
            },
            "development": {
              "optimization": false,
              "baseHref": "/"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "port": 4200
          },
            "configurations": {
            "development": {
              "buildTarget": "task-rating-frontend:build:development"
            },
            "production": {
              "buildTarget": "task-rating-frontend:build:production"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
